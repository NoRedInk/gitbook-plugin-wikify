{
  "name": "gitbook-plugin-wikify",
  "version": "1.0.0",
  "description": "GitBook plugin that makes your book wiki-like",
  "repository": {
    "type": "git",
    "url": "https://github.com/ento/gitbook-plugin-wikify.git"
  },
  "main": "lib/index.js",
  "bin": {
    "gitbook-autoindex": "./bin/gitbook-autoindex.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "ava"
  },
  "engines": {
    "gitbook": ">3.0.0"
  },
  "author": "Marica Odagaki",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@textlint/linter-formatter": "^3.0.4",
    "bash-color": "0.0.4",
    "bluebird": "^3.4.6",
    "cheerio": "^0.22.0",
    "fs-extra": "^1.0.0",
    "gitbook-cli": "^2.3.0",
    "glob": "^7.1.1",
    "interop-require": "^1.0.0",
    "is-file": "^1.0.0",
    "lodash": "^4.17.2",
    "minimatch": "^3.0.4",
    "test-anything-protocol": "^0.2.1",
    "yargs": "^6.4.0"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.3",
    "colors": "^1.0.3",
    "eslint": "^3.12.2",
    "gitbook-tester": "ento/gitbook-tester#testing-external-command",
    "tap-parser": "^3.0.4",
    "unique-temp-dir": "^1.0.0",
    "winston": "^2.3.0",
    "winston-memory": "^0.1.0"
  },
  "gitbook": {
    "properties": {
      "lintOutput": {
        "type": "string",
        "default": null,
        "description": "Output orphaned pages in jslint's XML format"
      },
      "lintExcludePatterns": {
        "type": "array",
        "items": {"type": "string"},
        "default": [],
        "description": "Glob patterns to exclude when checking orphans"
      }
    }
  }
}
